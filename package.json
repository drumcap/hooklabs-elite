{
  "name": "elite-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run __tests__/unit",
    "test:integration": "vitest run __tests__/integration",
    "test:components": "vitest run __tests__/components",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "docker:build": "docker build -t hooklabs-elite .",
    "docker:run": "docker run -p 3000:3000 hooklabs-elite",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "lighthouse": "lighthouse http://localhost:3000 --output-path=./reports/lighthouse.html --view",
    "security-audit": "npm audit --audit-level high",
    "deps:check": "npm outdated",
    "deps:update": "npx npm-check-updates -u && npm install",
    "perf:test": "node scripts/performance-test.js",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output-path=./reports/lighthouse-report.html --view",
    "perf:lighthouse:ci": "lhci autorun",
    "perf:bundle": "ANALYZE=true npm run build",
    "perf:load": "k6 run tests/performance/load-test.js",
    "perf:stress": "k6 run tests/performance/stress-test.js",
    "perf:api": "k6 run tests/performance/api-test.js",
    "perf:memory": "clinic doctor --on-port 'k6 run tests/performance/memory-test.js' -- npm start",
    "perf:e2e": "playwright test tests/performance/",
    "perf:regression": "node scripts/performance-regression.js",
    "perf:report": "node scripts/performance-report.js",
    "perf:baseline": "node scripts/set-performance-baseline.js",
    "perf:all": "npm run perf:load && npm run perf:stress && npm run perf:api && npm run perf:e2e"
  },
  "dependencies": {
    "@clerk/backend": "^2.4.1",
    "@clerk/nextjs": "^6.24.0",
    "@clerk/themes": "^2.2.55",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^8.55.0",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-table": "^8.21.3",
    "@upstash/redis": "^1.35.1",
    "@vercel/analytics": "^1.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.25.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.3",
    "lucide-react": "^0.525.0",
    "mixpanel-browser": "^2.56.0",
    "motion": "^12.23.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-use-measure": "^2.1.7",
    "recharts": "^3.1.2",
    "sonner": "^2.0.6",
    "svix": "^1.69.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "web-vitals": "^4.2.4",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/mixpanel-browser": "^2.49.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "clinic": "^13.1.0",
    "happy-dom": "^18.0.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "k6": "^0.0.0",
    "lighthouse": "^12.2.1",
    "msw": "^2.11.1",
    "playwright": "^1.55.0",
    "puppeteer": "^23.14.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
